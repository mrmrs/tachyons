{
  "name": "tachyons",
  "version": "4.9.0",
  "private": true,
  "description": "Custom styles for Buoy Symptoms A-Z. Based off Tachyons.",
  "author": "Buoy Dev Team",
  "style": "css/tachyons.min.css",
  "main": "css/tachyons.css",
  "files": [
    "css",
    "src"
  ],
  "repository": "tachyons-buoy",
  "keywords": [
    "css",
    "oocss",
    "postcss",
    "functional css",
    "design",
    "responsive",
    "performance"
  ],
  "license": "MIT",
  "devDependencies": {
    "copy-files": "^0.1.0",
    "immutable-css-cli": "^1.1.1",
    "normalize.css": "^7.0.0",
    "tachyons-modules": "^1.1.10",
    "tachyons-cli": "^1.0.11",
    "watch": "^1.0.2"
  },
  "scripts": {
    "start": "npm run build:watch",
    "mutations": "immutable-css src/tachyons.css --strict",
    "build": "npm run build:css && npm run build:minify",
    "build:css": "tachyons src/tachyons.css > css/tachyons.css",
    "build:minify": "tachyons src/tachyons.css -m > css/tachyons.min.css",
    "build:watch": "watch \"npm run build\" ./src/"
  }
}
