{
  "name": "@buoy/symptomcss",
  "version": "5.9.1",
  "private": true,
  "description": "Custom styles for Buoy Symptoms A-Z. Based off Tachyons.",
  "author": "Buoy Team <engineering@buoyhealth.com>",
  "style": "css/symptoms.min.css",
  "main": "css/symptoms.css",
  "files": [
    "css",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BuoyHealth/symptomcss.git"
  },
  "keywords": [
    "css",
    "oocss",
    "postcss",
    "functional css",
    "design",
    "responsive",
    "performance"
  ],
  "license": "MIT",
  "devDependencies": {
    "copy-files": "^0.1.0",
    "immutable-css-cli": "^1.1.1",
    "normalize.css": "^7.0.0",
    "tachyons-modules": "^1.1.10",
    "tachyons-cli": "^1.0.11",
    "watch": "^1.0.2"
  },
  "scripts": {
    "start": "npm run build:watch",
    "mutations": "immutable-css src/symptoms.css --strict",
    "build": "npm run build:css && npm run build:minify",
    "build:css": "tachyons src/symptoms.css > css/symptoms.css",
    "build:minify": "tachyons src/symptoms.css -m > css/symptoms.min.css",
    "build:watch": "watch \"npm run build\" ./src/"
  }
}
